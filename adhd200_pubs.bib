journal = {ArXiv e-prints},
keywords = {arxiv},
mendeley-tags = {Data Science},
month = {dec},
title = {{Correlation between graphs with an application to brain networks analysis}},
url = {http://arxiv.org/abs/1512.06830},
year = {2015}
}
